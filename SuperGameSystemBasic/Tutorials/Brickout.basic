REM leave the game when escape is pressed
WHILE NOT KEYPRESSED(@KEYESC)
  IF SUM(bricks) = 0 THEN
    Reset()
  ENDIF                   
  IF started THEN
    UpdateBall()
  ENDIF              
  FOR y = 0 TO GETROWS()-1
    PRINT(bg,@LIGHT_GREEN,@BLACK)    
  NEXT y
  UpdatePlayer()
  DrawBricks()
  DrawPlayer()
  YEILD(10)
  BLIT
WEND 

FUNCTION CheckBallCollisions()
  FOR i = 0 TO 4
    FOR j = 0 TO 7 
      IF bricks[(i*8)+j] THEN
        IF ROUNDDOWN(by) = i THEN
          IF bx>=(j*10) AND bx <= ((j*10)+10) THEN 
            bricks[(i*8)+j] = FALSE
            bvy = bvy * -1 
            SOUND(400,16384,100,@NOISE)
          ENDIF
        ENDIF        
      ENDIF
    NEXT j         
  NEXT i  
ENDFUNCTION

FUNCTION UpdatePlayer()    
  IF KEYPRESSED(@KEYLEFT) THEN
    px = MAX(px-1,pw/2) 
    IF NOT started THEN
      bx = px
    ENDIF
  ELSEIF KEYPRESSED(@KEYRIGHT) THEN
    px = MIN(px+1,GETCOLUMNS()-(pw/2))
    IF NOT started THEN
      bx=px
    ENDIF
  ELSEIF KEYPRESSED(@SPACE) OR KEYPRESSED(@KEYENTER) THEN
    IF NOT started THEN
      started = TRUE
      bvy = -0.1 
      SOUND(400,16384,100,@SAWTOOTH)
    ENDIF
    IF px> GETCOLUMNS()/2 THEN
      bvx = (px - (GETCOLUMNS()/2)) / (GETCOLUMNS()/2)
    ELSE                                        
      bvx = (1-(px/(GETCOLUMNS()/2))) * -1
    ENDIF
  ENDIF
ENDIF 
ENDIF
ENDFUNCTION

FUNCTION UpdateBall()      
  bx = bx + bvx
  by = by + bvy                       
  IF by >= GETROWS() THEN
    ClearBricks()
  ENDIF        
  IF bx <= 0 OR bx >= GETCOLUMNS() THEN
    bvx = bvx * -1
  ENDIF     
  IF by <= 0 THEN
    bvy = bvy * -1
  ENDIF                                   
  IF ROUNDDOWN(by)+1 = py AND bvy > 0 THEN
    IF bx >= (px-(pw/2)-1) AND bx <= (px+(pw/2)+1) THEN
      bvy = MAX(bvy+0.01,0) * -1 REM slowly make it harder
      SOUND(400,16384,100,@SAWTOOTH)
    ENDIF 
  ENDIF  
  IF by <= 5 THEN  
    CheckBallCollisions()
  ENDIF
ENDFUNCTION

FUNCTION ClearBricks()
  FOR i = 0 TO COUNT(bricks)-1
    bricks[i] = 0
  NEXT i
ENDFUNCTION

FUNCTION DrawBricks()   
  FOR i = 0 TO 4
    FOR j = 0 TO 7 
      IF bricks[(i*8)+j] THEN
        LINE(j*10,i,(j*10)+10,i,i+1)
      ENDIF
    NEXT j         
  NEXT i  
ENDFUNCTION               

FUNCTION DrawPlayer()
  LINE(px-(pw/2),py,px+(pw/2),py,15)
  CURSORXY(bx,by)
  PRINTG(@BULLET,@YELLOW)
ENDFUNCTION         

FUNCTION Reset()
  CLEAR()
  FILL(0,0,80,splash)  
  CURSORXY(0,1)
  PRINT("Resetting Bricks")
  FOR i = 0 TO COUNT(bricks)-1
    bricks[i] = TRUE
  NEXT i
  PRINT(" Done!\nResetting Player")  
  py = GETROWS()-3
  px = (GETCOLUMNS()/2) - (pw/2)  
  PRINT(" Done!\nResetting Ball")
  started = FALSE
  bx = px
  by = py-1   
  bvx = 0
  bvy = 0
  PRINT(" Done!\nPress any key to Play!")
  BLIT
  WHILE NOT KEYPRESSED()
    YEILD(10)
  WEND      
  YEILD(200)               
  CLEAR()
ENDFUNCTION  
