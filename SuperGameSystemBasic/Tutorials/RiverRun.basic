GLYPHSET(@BOAT,boatGlyph)

REM states 0 = menu 1 = playing 2 = dying
VAR state = 0

WHILE state >= 0 OR (NOT KEYPRESSED(@KEYESC))
  IF state = 0 THEN
    MENU()  
  ELSEIF state = 1 THEN
    PLAY()
  ELSE
    DIE()
  ENDIF     
  YEILD(tDiff)
  BLIT            
WEND   
END                          

FUNCTION MENU()  
  FILL(0,0,80,splash)
  CURSORXY(2,2)
  PRINT("Press any key to play or ESC to exit",15,2)
  IF KEYPRESSED(@KEYESC) THEN
    state = -1   
  ELSEIF KEYPRESSED() THEN
    state = 1
  ENDIF
ENDFUNCTION

FUNCTION DIE()  
  IF started THEN
    TIMER(1,0)
    started = FALSE 
    SOUND(400,16384,1000,@NOISE)
  ENDIF
  IF TIMER(1) < 1000 THEN   
    UpdateSides()
    CLEAR(1)
    DrawSides()             
    CURSORXY(boatx,18)
    
    IF boatx<=lSide[18] OR boatx >= rSide[18] THEN 
      temp = colors[18]   
    ELSE
      temp = 1
    ENDIF
    IF odd = 1 THEN
      PRINTG(@STAR,@RED,temp)
      odd = 0
    ELSE
      PRINTG(@STAR,@YELLOW,temp,@VERTICAL)
      odd = 1
    ENDIF
  ELSE
    state = 0
    tDiff = 50
  ENDIF
ENDFUNCTION

FUNCTION PLAY()  
  IF KEYPRESSED(@KEYESC) THEN
    state = 2          
  ELSE
    IF NOT started THEN
      InitialiseSides()
      boatx = 40
      tDiff = 100
      eDiff = 14  
      jDiff = 2
      score = 0
      started = TRUE
    ENDIF
    UpdateSides()  
    score  = score + 1
    CLEAR(1)   
    DrawSides()
    
    IF KEYPRESSED(@KEYLEFT) THEN
      boatx = boatx - 1 
    ELSEIF KEYPRESSED(@KEYRIGHT) THEN
      boatx = boatx + 1 
    ENDIF                      
    IF boatx<=lSide[18] OR boatx >= rSide[18] THEN 
      state = 2
    ENDIF 
    IF score%25 = 0 THEN
      tDiff = MAX(tDiff-4,1)
    ENDIF      
    IF score%50 = 0 THEN
      jDiff = MIN(jDiff+1,7)
    ENDIF        
    IF score%200 = 0 THEN
      eDiff = MAX(eDiff-1,5)
    ENDIF
    CURSORXY(boatx,18)  
    PRINTG(@BOAT,12,1)  
    LINE(0,0,GETCOLUMNS(),0,15)
    CURSORXY(2,0) 
    PRINT("SCORE: " + score,0,15)
  ENDIF
ENDFUNCTION  

FUNCTION UpdateSides()    
  FOR i = COUNT(lSide)-1 TO 1 
    lSide[i] = lSide[i-1]
    rSide[i] = rSide[i-1]      
    colors[i] = colors[i-1]       
  NEXT i   
  
  IF colors[1] = 2 THEN
    colors[0] = 10
  ELSE       
    colors[0] = 2
  ENDIF            
  
  IF RAND(0,10) > 2 THEN         
    lSide[0] = MAX(MIN(lSide[1] + RAND(1,jDiff),rSide[1]-eDiff),0)
    rSide[0] = MIN(MAX(rSide[1] - RAND(1,jDiff),lSide[1]+eDiff),GETCOLUMNS()-2)    
  ELSE
    lSide[0] = MAX(MIN(lSide[1] - RAND(1,jDiff),rSide[1]-eDiff),0) 
    rSide[0] = MIN(MAX(rSide[1] + RAND(1,jDiff),lSide[1]+eDiff),GETCOLUMNS()-2)         
  ENDIF 
  temp = RAND(2) 
  IF temp = 0 THEN 
    rSide[0] = MIN(rSide[0]+1,GETCOLUMNS()-2)   
    lSide[0] = MIN(lSide[0]+1,rSide[0]-eDiff)
  ELSEIF temp = 1 THEN
    lSide[0] = MAX(lSide[0] -1,0) 
    rSide[0] = MAX(rSide[0] -1,lSide[0]+eDiff)  
  ENDIF
ENDFUNCTION  

FUNCTION DrawSides()    
  FOR i = 0 TO COUNT(lSide)-1
    LINE(0,i,lSide[i],i,colors[i])
    LINE(rSide[i],i,GETCOLUMNS(),i,colors[i]) 
  NEXT i            
ENDFUNCTION  

FUNCTION InitialiseSides() 
  FOR i = 0 TO COUNT(lSide)-1
    lSide[i] = 0 
    rSide[i] = GETCOLUMNS()-1
    IF i%2 = 0 THEN
      colors[i] = 2 
    ELSE
      colors[i] = 10 
    ENDIF          
  NEXT i        
ENDFUNCTION